            android:textColor="@color/black" />

        <TextView
            android:id="@+id/value_tolling_distance"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/key_tolling_cost"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Cost:"
            android:textColor="@color/black" />


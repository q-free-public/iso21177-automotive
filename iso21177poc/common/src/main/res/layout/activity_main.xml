        <TextView
            android:id="@+id/key_location_head"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="head:"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/value_location_head"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/key_location_velo"

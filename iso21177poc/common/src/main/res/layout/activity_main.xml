            android:text="Timestamp:"
            android:textColor="@color/black" />

        <TextView
            android:id="@+id/value_location_timestamp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/key_location_lat"
            android:layout_width="match_parent"
